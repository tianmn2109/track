@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="initial-scale=1.0, width=device-width, maximum-scale=1.0, user-scalable=no" />

    <!-- Demo Styles -->
    <style type="text/css">
        * {
            margin: 0;
            padding: 0;
            outline: 0;
        }

        html, body {
            height: 100%;
            width: 100%;
            background: #F2F2F2;
        }

        body, a {
            font: normal 16px Helvetica, Verdana, Geneva, sans-serif;
            color: #3F3F3F;
        }

        .con {
            min-height: 100%;
            height: auto !important;
            height: 100%;
            margin: 0 auto -30px;
            width:100%;
            
        }

            .con:after {
                content: '';
                display: block;
                height: 30px;
                clear: both;
            }

        .footer {
            height: 12px;
            padding: 8px 0;
            background: #FFF;
            border-top: 1px solid #51C1F1;
            font-size: 12px;
            text-align: center;
    
    </style>
    

    <script src="http://code.jquery.com/jquery-1.9.1.js" type="text/javascript"></script>
    <script src="http://code.highcharts.com/highcharts.js" type="text/javascript"></script>
    <script src="http://code.highcharts.com/highcharts-more.js"></script>
    <script src="http://code.highcharts.com/modules/exporting.js" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/features.js")" type=" text/javascript"></script>
    <script src="@Url.Content("~/Scripts/drawDashboard.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/moment.js")"></script>

    <link rel="stylesheet" href="@Url.Content("~/Content/navigataur.css")" />

    <link rel="stylesheet" href="@Url.Content("~/Content/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/application.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/arrowsandboxes.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/format.css")" />


    

    <title>Stage</title>
</head>
<body>
    
    <div class="con" >
        <div class="header clearfix"  >
            <h1>Stage</h1>

            <div class="nav" >

                <input type="checkbox" id="toggle" />
                <label for="toggle" class="toggle" onclick()></label>
                <ul class="menu">
                    <li><a href="./stage">Stage</a></li>
                    <li><a href="./schedule">Schedule</a></li>
                    <li><a href="./resource">Resource</a></li>
                    <li><a href="./other">Other</a></li>
                </ul>
            </div><!-- End of Navigation -->

        </div><!-- End of Header -->
        
        <div id="filters"  style="width:80%; text-align: left;  margin:0 auto;">
            <div id="filter1" >
                <label>Filter</label>
                <select id="stageBox" onchange="getFeatureStage()" style="width:100px; height:35px; ">
                    <option value="1">All</option>
                    <option value="2">New</option>
                    <option value="3">Active</option>
                    <option value="4">Closed</option>
                </select>


            </div>
        </div>
            <div id="features" name="features" style="width:80%; text-align: center; margin:0 auto;">

                <script type="text/javascript" defer="defer">
                    getFeatureStage();
                </script>

            </div>
        </div><!-- End of Container -->

    
   
    <div class="footer">
        <p>&copy; 2015 Track the project. | Created by Mengnan Tian</p>
    </div>

</body>
</html>